open_creat_results open(open_params) = 1; 	// path, access_flag
open_creat_results creat(creat_params) = 2; // path
read_results read(read_params) = 3; 		// fd, count
write_results write(write_params) = 4; 		// fd, buf, buf_size
lseek_results lseek(lseek_params) = 5;		// fd, offset, whence_flag
close_results close(close_params) = 6;		// fd

rpcgen mynfs.x -a
make
sudo ./mynfs_server
./mynfs_client localhost